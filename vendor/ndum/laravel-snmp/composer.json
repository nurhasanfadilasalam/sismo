{
    "name": "ndum/laravel-snmp",
    "description": "A FreeDSx/SNMP SNMP-Wrapper for Laravel",
    "type": "laravel",
    "require": {
        "php": "^7.1",
        "freedsx/snmp": "^0.4.0",
        "illuminate/support": "~5.5|~6.0|~7.0|~8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas D",
            "email": "nd@nidum.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ndum\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ndum\\Laravel\\SnmpServiceProvider"
            ],
            "aliases": {
                "snmp": "Ndum\\Laravel\\Facades\\Snmp"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    }
}
